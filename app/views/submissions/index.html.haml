#list_test
	- @categories.each do |category|
		%ul
			%li.header= category.name
			- category.quizzes.each do |quiz|
				%li
					= link_to quiz.name, new_submission_path(:quiz_id => quiz.id)
					= link_to 'last submission', @submission if @submission = quiz.last_submission_for_session_id(@session_id)
